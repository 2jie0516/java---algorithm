import java.util.Arrays;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        long result = 0;
        long cnt = 1;
        long value = 10;
        long num = sc.nextLong();

        for(long i=1;i<=num;i++){
            if(i >= value) {
                value *= 10;
                cnt ++;
            }
            result += cnt;
        }
        System.out.println(result);
    }

}

